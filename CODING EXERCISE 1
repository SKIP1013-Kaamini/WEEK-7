package CLASS;

class Exercise1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int  i = 1;
		int j = 1;
		while (i<5){
		      System.out.println( " i = " +i);
		     
		      i= i + 1;   
		        } 
		
		do {
			      System.out.println( " j = " +j);
			     
			      j= j + 1;
		}while(j<5);

	}

}
